<% include ../partials/header %>

<!--<link rel="stylesheet" type="text/css" href="../public/css/bootstrap.css">-->


<div class="container">
    <header class="jumbotron">
        <div class="container">
            <div>
                <h1><i class="fa fa-file-text-o" aria-hidden="true"></i> <%= job.name %></h1>
                <p><%= job.created.toDateString() %></p>
            </div>
        </div>
    </header>



    <div class="container">
        <div class="row">
            <ul class="list-group col-md-11">
                <% var index = 0; job.smslist.forEach(function(sms){ %>
                <li class="list-group-item">
                    <a href="/jobs/<%= job._id %>/smslist/<%= sms._id %>">
                        <div>
                            <%= index +1 %> )
                            <%= sms.name%>
                            <span class="badge" style="float: right"><%= sms.phonenumber %></span>
                        </div>
                    </a>
                </li>
                <% index++ }); %>
            </ul>
        </div>
    </div>

</div>


<% include ../partials/footer %>